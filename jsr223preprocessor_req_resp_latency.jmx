            <stringProp name="script">import java.text.SimpleDateFormat
import java.util.Date


// Define the desired date format with milliseconds
def dateFormat = new SimpleDateFormat(&quot;hh:mm:ss.SSS dd:MM:yyyy&quot;)

// Get the request and response timestamps
def requestTimestamp = prev.getStartTime()
def responseTimestamp = prev.getEndTime()


// Get the latency
def latency = prev.getLatency()


// Get the request number from the sampler label
def requestNumber = prev.getSampleLabel()


// Format the timestamps
def formattedRequestTimestamp = dateFormat.format(new Date(requestTimestamp))
def formattedResponseTimestamp = dateFormat.format(new Date(responseTimestamp))


// Log the data
log.info(&quot;Request Number: &quot; + requestNumber)
log.info(&quot;Formatted Request Timestamp: &quot; + formattedRequestTimestamp)
log.info(&quot;Formatted Response Timestamp: &quot; + formattedResponseTimestamp)
log.info(&quot;Latency: &quot; + latency + &quot; ms&quot;)


// Write to CSV file
def csvData = requestNumber + &quot;,&quot; + formattedRequestTimestamp + &quot;,&quot; + formattedResponseTimestamp + &quot;,&quot; + latency
new File(&apos;output.csv&apos;) &lt;&lt; csvData + System.getProperty(&apos;line.separator&apos;)


</stringProp>
